#include <unistd.h>
#include "main.h"

/**
 * main - check the code
 *
 * DESCRIPTION: Write a program that prints _putchar, followed by a new line.
 *
 * Return: Always 0.
 */
int main(void)
{
	int i = 0;
	char arr[] = "_putchar";

	for (i = 0; i < arr[i]; i++)
	{
		_putchar(arr[i]);
	}
	_putchar('\n');
	return (0);
}
